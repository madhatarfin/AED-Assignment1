    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        employee.setEname(txtEname.getText());
        employee.setEno(txtEno.getText());
        employee.setAge(txtAge.getText());
        employee.setGen(txtGen.getText());
        employee.setSdate(txtSdate.getText());
        employee.setLevel(txtLevel.getText());
        employee.setTeaminfo(txtTeamInfo.getText());
        employee.setTitle(txtTitle.getText());
        employee.setCellphone(txtCellPhone.getText());
        employee.setEmailid(txtEmailid.getText());
        
        JOptionPane.showMessageDialog(this, "Employee Information Saved.");
    }                                        

